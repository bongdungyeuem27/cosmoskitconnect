{
  "name": "@cosmoskitconnect/utils",
  "description": "Utilities for WalletConnect Protocol",
  "version": "0.1.2",
  "author": "WalletConnect, Inc. <walletconnect.com>",
  "homepage": "https://github.com/walletconnect/walletconnect-monorepo/",
  "license": "Apache-2.0",
  "main": "dist/index.cjs.js",
  "source": "./src/index.ts",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "wallet",
    "walletconnect"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prepare": "bun run build",
    "build:pre": "bun run clean",
    "build:types": "tsc",
    "build:source": "rollup --config rollup.config.js",
    "build": "bun run build:pre; bun run build:source; bun run build:types",
    "test:pre": "rm -rf ./test/test.db",
    "test:run": "vitest run --dir test",
    "test": "bun run test:pre; bun run test:run",
    "test:ignoreUnhandled": "bun run test:pre; bun run test:run -- --dangerouslyIgnoreUnhandledErrors",
    "lint": "eslint -c '../../.eslintrc' --fix './src/**/*.ts'",
    "prettier": "prettier --check '{src,test}/**/*.{js,ts,jsx,tsx}'"
  },
  "dependencies": {
    "@cosmoskitconnect/jsonrpc-utils": "0.1.2",
    "@cosmoskitconnect/keyvaluestorage": "0.1.2",
    "@cosmoskitconnect/relay-api": "0.1.2",
    "@cosmoskitconnect/relay-auth": "0.1.2",
    "@cosmoskitconnect/safe-json": "0.1.2",
    "@cosmoskitconnect/time": "0.1.2",
    "@cosmoskitconnect/types": "0.1.2",
    "@cosmoskitconnect/window-getters": "0.1.2",
    "@cosmoskitconnect/window-metadata": "0.1.2",
    "@ethersproject/hash": "5.7.0",
    "@ethersproject/transactions": "5.7.0",
    "@stablelib/chacha20poly1305": "1.0.1",
    "@stablelib/hkdf": "1.0.1",
    "@stablelib/random": "1.0.2",
    "@stablelib/sha256": "1.0.1",
    "@stablelib/x25519": "1.0.3",
    "detect-browser": "5.3.0",
    "elliptic": "6.6.1",
    "query-string": "7.1.3",
    "uint8arrays": "3.1.0"
  },
  "devDependencies": {
    "@cosmoskitconnect/jsonrpc-types": "0.1.2",
    "@types/elliptic": "6.4.18",
    "@types/lodash.isequal": "4.5.6"
  },
  "gitHead": "bf4e911852bf89c528d98e2d9518766b9d50b038"
}
